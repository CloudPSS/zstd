{
  "npm.packageManager": "pnpm",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "files.eol": "\n",
  "editor.tabSize": 2,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": ["napi", "zstd"],
  "files.associations": {
    "**/.git/config": "gitconfig",
    "**/.xmake/**/*": "lua",
    "**/build/.deps/**/*.d": "lua",
    "*.caddy": "caddyfile",
    "*.cu": "cpp",
    "*.cuh": "cpp",
    "*.js.snap": "javascript",
    "*.pscx": "xml",
    "*.pslx": "xml",
    "*.resjson": "json",
    ".yarnrc": "properties",
    "Caddyfile": "caddyfile",
    "algorithm": "cpp",
    "functional": "cpp",
    "optional": "cpp",
    "system_error": "cpp",
    "type_traits": "cpp",
    "xtr1common": "cpp",
    "xutility": "cpp"
  }
}
